{
  "version": 2,
  "framework": null,
  "functions": {
    "api/**/*.py": {
      "memory": 128,
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/main.py"
    }
  ],
  "env": {},
  "builds": [
    {
      "src": "api/main.py",
      "use": "@vercel/python",
      "config": {
        "includeFiles": ["api/**"]
      }
    }
  ]
}
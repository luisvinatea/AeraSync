{
  "version": 2,
  "framework": null,
  "routes": [
    {
      "src": "/health",
      "dest": "api/main.py"
    },
    {
      "src": "/compare",
      "dest": "api/main.py"
    },
    {
      "src": "/api/(.*)",
      "dest": "api/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "api/main.py"
    }
  ],
  "env": {
    "CORS_ORIGINS": "https://aerasync.vercel.app,http://localhost:3000"
  },
  "builds": [
    {
      "src": "api/main.py",
      "use": "@vercel/python"
    }
  ]
}

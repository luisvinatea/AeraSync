{
  "version": 2,
  "framework": null,
  "routes": [
    {
      "src": "/health",
      "dest": "/api/main.py",
      "methods": ["GET"],
      "headers": {
        "cache-control": "no-cache"
      }
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/main.py"
    }
  ],
  "env": {
    "CORS_ORIGINS": "https://aerasync-web.vercel.app"
  },
  "builds": [
    {
      "src": "api/main.py",
      "use": "@vercel/python",
      "config": {
        "includeFiles": ["api/**"],
        "maxDuration": 30,
        "memory": 256
      }
    }
  ]
}

{
  "version": 2,
  "buildCommand": "chmod +x fix_build.sh && ./fix_build.sh",
  "outputDirectory": "public",
  "trailingSlash": false,
  "cleanUrls": true,
  "rewrites": [{ "source": "/(.*)", "destination": "/index.html" }],
  "headers": [
    {
      "source": "/(.+\\.[a-zA-Z0-9]+)$",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}

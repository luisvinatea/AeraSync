{
  "version": 2,
  "framework": null,
  "builds": [
    {
      "src": "pubspec.yaml",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build/web",
        "buildCommand": "flutter pub get && flutter build web --release --dart-define=API_URL=https://aerasync-api.vercel.app"
      }
    }
  ],
  "routes": [
    {
      "src": "/privacy.html",
      "dest": "/privacy.html"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/icons/(.*)",
      "dest": "/icons/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "API_URL": "https://aerasync-api.vercel.app"
  }
}